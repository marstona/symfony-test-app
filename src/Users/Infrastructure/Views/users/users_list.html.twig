{% extends 'base.html.twig' %}

{% block subtitle %}
  <h1 class="title">Users list</h1>
{% endblock subtitle %}

{% block body %}
  <div class="container">
    <table class="table">
      <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
      </tr>
      </thead>
      <tbody>
      {% for user in users %}
        <tr>
          <td>{{ user.id }}</td>
          <td>{{ user.email }}</td>
        </tr>
      {% endfor %}
      </tbody>
    </table>
  </div>
{% endblock body %}